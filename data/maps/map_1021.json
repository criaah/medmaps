        "text": "DEFINICIÓN",
        "guid": "",
        "children": []
      },
      {
        "id": "8",
        "parent": "0",
        "text": "↳ Enfermedad → ↓\nresistencia esquelética",
        "guid": "",
        "children": []
      },
      {
        "id": "9",
        "parent": "0",
        "text": "↳ → ↑ riesgo fractura",
        "guid": "",
        "children": []
      },
      {
        "id": "10",
        "parent": "0",
        "text": "CRITERIOS\nDIAGNÓSTICOS\n[requiere ≥1]",
        "guid": "",
        "children": []
      },
      {
        "id": "11",
        "parent": "0",
        "text": "1. Fractura\nosteoporótica:",
        "guid": "",
        "children": []
      },
      {
        "id": "12",
        "parent": "0",
        "text": "•\nEspontánea/trauma\nbaja energía",
        "guid": "",
        "children": []
      },
      {
        "id": "13",
        "parent": "0",
        "text": "• Edad >40 años",
        "guid": "",
        "children": []
      },
      {
        "id": "14",
        "parent": "0",
        "text": "• Sitios típicos:\nvértebra/cadera/antebrazo/húmero/tobillo/pelvis",
        "guid": "",
        "children": []
      },
      {
        "id": "15",
        "parent": "0",
        "text": "• Exclusión:\ncara/cráneo/falanges/ortejos",
        "guid": "",
        "children": []
      },
      {
        "id": "16",
        "parent": "0",
        "text": "2. Densitometría ósea\n(DXA) anormal:",
        "guid": "",
        "children": []
      },
      {
        "id": "17",
        "parent": "0",
        "text": "↳ Criterios OMS 1994",
        "guid": "",
        "children": []
      },
      {
        "id": "18",
        "parent": "0",
        "text": "↳ Medición: columna\nlumbar/cuello femoral/cadera\ntotal",
        "guid": "",
        "children": []
      },
      {
        "id": "19",
        "parent": "0",
        "text": "↳ T-score →\ncomparación con:",
        "guid": "",
        "children": []
      },
      {
        "id": "20",
        "parent": "0",
        "text": "• Persona sana",
        "guid": "",
        "children": []
      },
      {
        "id": "21",
        "parent": "0",
        "text": "• 30 años",
        "guid": "",
        "children": []
      },
      {
        "id": "22",
        "parent": "0",
        "text": "• Mismo sexo/etnia",
        "guid": "",
        "children": []
      },
      {
        "id": "23",
        "parent": "0",
        "text": "VALORES T-SCORE",
        "guid": "",
        "children": []
      },
      {
        "id": "24",
        "parent": "0",
        "text": "↳ Normal: >-1 DS",
        "guid": "",
        "children": []
      },
      {
        "id": "25",
        "parent": "0",
        "text": "↳ Osteopenia:\n-1 a -2.5 DS",
        "guid": "",
        "children": []
      },
      {
        "id": "26",
        "parent": "0",
        "text": "↳\nOsteoporosis:\n<-2.5 DS",
        "guid": "",
        "children": []
      },
      {
        "id": "27",
        "parent": "0",
        "text": "↳ Osteoporosis grave: <-2.5\nDS + fractura fragilidad",
        "guid": "",
        "children": []
      },
      {
        "id": "28",
        "parent": "0",
        "text": "FRACTURAS\nVERTEBRALES",
        "guid": "",
        "children": []
      },
      {
        "id": "29",
        "parent": "0",
        "text": "↳ Prevalencia\nLatinoamérica [Estudio\nLAVOS]:",
        "guid": "",
        "children": []
      },
      {
        "id": "30",
        "parent": "0",
        "text": "• >50 años: 11.8%",
        "guid": "",
        "children": []
      },
      {
        "id": "31",
        "parent": "0",
        "text": "• 50-59 años: 8.3%",
        "guid": "",
        "children": []
      },
      {
        "id": "32",
        "parent": "0",
        "text": "• >80 años: 37%",
        "guid": "",
        "children": []
      },
      {
        "id": "33",
        "parent": "0",
        "text": "FACTORES\nRIESGO\nFRACTURA",
        "guid": "",
        "children": []
      },
