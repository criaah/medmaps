        "children": [
          {
            "id": "71",
            "parent": "70",
            "text": "5.1. Organización\nCortical",
            "guid": "W77t237_g6V0990zU8UFZQ",
            "children": [
              {
                "id": "72",
                "parent": "71",
                "text": "Corteza\nsomatosensitiva\nprimaria (S1):",
                "guid": "M7v3GnmzMMAIHCPcT-iK4Q",
                "children": [
                  {
                    "id": "73",
                    "parent": "72",
                    "text": "Localizada en el giro postcentral",
                    "guid": "AZVhI3iNT51kuHZAGgcbJg",
                    "children": []
                  },
                  {
                    "id": "74",
                    "parent": "72",
                    "text": "Organización somatotópica precisa",
                    "guid": "TeTnwyjYSXZRz9ixCbFFsw",
                    "children": []
                  },
                  {
                    "id": "75",
                    "parent": "72",
                    "text": "Procesamiento inicial de información táctil y propioceptiva",
                    "guid": "JsUb6Q1etb10q30ZbJaaog",
                    "children": []
                  },
                  {
                    "id": "76",
                    "parent": "72",
                    "text": "División en áreas especializadas\n(3a, 3b, 1 y 2)",
                    "guid": "BhnTXFt0cb80FYBEfHMNHQ",
                    "children": []
