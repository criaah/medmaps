    "text": "INCIDENTALOMA\nHIPOFISIARIO (IH)",
    "guid": "Caq2tTeLVCIsljQ4BGRDQw",
    "children": [
      {
        "id": "1",
        "parent": "0",
        "text": "DEFINICIÓN",
        "guid": "",
        "children": [
          {
            "id": "2",
            "parent": "1",
            "text": "→ Hallazgo lesiones hipofisarias",
            "guid": "fbHrYFH7dBMgaJ0DQ3uvpQ",
            "children": []
          },
          {
            "id": "3",
            "parent": "1",
            "text": "→ Detectado en estudio radiológico craneal",
            "guid": "MkaKymJJZEEfTInUCbs9Cg",
            "children": []
          },
          {
            "id": "4",
            "parent": "1",
            "text": "→ Sin relación con enf. hipofisarias",
            "guid": "XO-sinv69lwAd90BU5Odgw",
            "children": []
          }
        ]
      },
      {
        "id": "5",
        "parent": "0",
        "text": "PREVALENCIA",
        "guid": "",
        "children": [
          {
            "id": "6",
            "parent": "5",
            "text": "→ Autopsias: 1.5-31.1%",
            "guid": "Q9WhxHgbPKcl4pp_FwgrnA",
            "children": []
          },
          {
            "id": "7",
            "parent": "5",
            "text": "→ Estudios radiológicos: 3.7-38. 5%",
            "guid": "RH-4jhZDt_5IjlJnYFrdfg",
            "children": []
          },
          {
            "id": "8",
            "parent": "5",
            "text": "→ Meta-análisis: 16. 7% promedio",
            "guid": "AoQSrh-1UEZiK30MRm-7UA",
            "children": []
          }
        ]
      },
      {
        "id": "9",
        "parent": "0",
        "text": "ETIOLOGÍA",
        "guid": "",
        "children": [
          {
            "id": "10",
            "parent": "9",
            "text": "Principal: adenomas hipofisiarios",
            "guid": "XjlpR0usBHlBXGqLZ-Qf_A",
            "children": []
          },
          {
            "id": "11",
            "parent": "9",
            "text": "Otras lesiones\n(infrecuentes)",
            "guid": "AzdYnG3ynsRsSGM_AOlD_A",
            "children": [
              {
                "id": "12",
                "parent": "11",
                "text": "→ Craneofaringeomas",
                "guid": "P-4S1wyxAEJEZPOhcjSdoQ",
                "children": []
              },
              {
                "id": "13",
                "parent": "11",
                "text": "→ Meningiomas",
                "guid": "bvpkhGOxfXZ779qsS-oRDg",
                "children": []
              },
              {
                "id": "14",
                "parent": "11",
                "text": "→ Metástasis",
                "guid": "X6xz0nxnt60ffa6SI4IVlg",
                "children": []
              },
              {
                "id": "15",
                "parent": "11",
                "text": "→ Quistes Rathke",
                "guid": "dIL0VEVgSRE6ikEyOQKs4g",
                "children": []
              },
              {
                "id": "16",
                "parent": "11",
                "text": "→ Tumores dermoides",
                "guid": "W6QBDwMYk5oZXYpgXigTvQ",
                "children": []
              },
              {
                "id": "17",
                "parent": "11",
                "text": "→ Tumores epidermoides",
                "guid": "Is3j4HuJB2wkl88NAQdNJw",
                "children": []
              },
              {
                "id": "18",
                "parent": "11",
                "text": "→ Hipofisitis",
                "guid": "RzUL5mX0OZho620jSmxkgg",
                "children": []
              },
              {
                "id": "19",
                "parent": "11",
                "text": "→ Aneurismas",
                "guid": "U-bYXFUz0GJLVah-E9TrNA",
                "children": []
              }
            ]
          }
        ]
      },
      {
        "id": "20",
        "parent": "0",
        "text": "PRESENTACIÓN\nCLÍNICA",
        "guid": "QM1OMmALIbNYEqjRCAJaGA",
        "children": [
          {
            "id": "21",
            "parent": "20",
            "text": "Microadenomas",
            "guid": "YeTQpA-6nB8GCYjVUN81KA",
            "children": [
              {
                "id": "22",
                "parent": "21",
                "text": "→ <1 cm",
                "guid": "c2wZlQH5Y4EcyUY3UxiNZw",
                "children": []
              },
              {
                "id": "23",
                "parent": "21",
                "text": "→ Mayoría de casos",
                "guid": "fmEbLjxG9Ml2mqL9cuQPgg",
                "children": []
              }
            ]
          },
          {
            "id": "24",
            "parent": "20",
            "text": "Macroadenomas",
            "guid": "Aac92jEk5DAr5rxlXUs-6Q",
            "children": [
              {
                "id": "25",
                "parent": "24",
                "text": "→ <0.5% casos",
                "guid": "ND13ykVERsU7c1KmVwtbqg",
                "children": []
              }
            ]
          }
        ]
      },
      {
        "id": "26",
        "parent": "0",
        "text": "DIAGNÓSTICO\nIMAGEN",
        "guid": "",
        "children": [
          {
            "id": "27",
            "parent": "26",
            "text": "RM + gadolinio:",
            "guid": "Rf9bS0D-FfRSbr6aGeYzqA",
            "children": [
              {
                "id": "28",
                "parent": "27",
                "text": "→ Mayor sensibilidad",
                "guid": "FjHmVh3EZxgtux7ceBa2-g",
                "children": []
              },
              {
                "id": "29",
                "parent": "27",
                "text": "→ Esp. microadenomas",
                "guid": "LX8DNzPEp7FI9ewjIOsczQ",
                "children": []
              },
              {
                "id": "30",
                "parent": "27",
                "text": "→ Imágenes hipointensas",
                "guid": "IgYnI2qeTTInAzXkI61k_Q",
                "children": []
              }
            ]
          },
          {
            "id": "31",
            "parent": "26",
            "text": "Características T1:",
            "guid": "Nb4LM2W_Mlp0A6o0NB8mYQ",
            "children": [
              {
                "id": "32",
                "parent": "31",
                "text": "→ No refuerzo con contraste",
                "guid": "G8MxYlLp8kkA-KPmUACpLA",
                "children": []
              }
            ]
          }
        ]
      },
      {
        "id": "33",
        "parent": "0",
        "text": "MANIFESTACIONES\nCLÍNICAS",
        "guid": "",
        "children": [
          {
            "id": "34",
            "parent": "33",
            "text": "SÍNTOMAS",
            "guid": "QBzDmzNsn_J8XKcyVk6p8Q",
            "children": [
              {
                "id": "35",
                "parent": "34",
                "text": "→ Mayoría asintomáticos",
                "guid": "HHcYP38eradg_eFYYnZzig",
                "children": []
              },
              {
                "id": "36",
                "parent": "34",
                "text": "→ Algunos adenomas\npresentan:",
                "guid": "QabHgUPGr25qAPkDWdUAkA",
                "children": [
                  {
                    "id": "37",
                    "parent": "36",
                    "text": "Hipersecreción hormonal",
                    "guid": "UTEHCioXxg5OZWDrfrAKQg",
                    "children": []
                  },
                  {
                    "id": "38",
                    "parent": "36",
                    "text": "Alt. campo visual",
                    "guid": "XdxtwBdbTQ4fmycPE5p48w",
                    "children": []
                  },
                  {
                    "id": "39",
                    "parent": "36",
                    "text": "Compromiso pares craneales",
                    "guid": "fRp_aBOe0UNHuZ9UHyCmiw",
                    "children": []
                  },
                  {
                    "id": "40",
                    "parent": "36",
                    "text": "Síntomas neurológicos",
                    "guid": "fj0edW681ThCzguIGgBP1w",
                    "children": []
                  }
                ]
              },
              {
                "id": "41",
                "parent": "34",
                "text": "→ Especial\natención:",
                "guid": "ADKl-hEM_dky0IfQHKm-OQ",
                "children": [
                  {
                    "id": "42",
                    "parent": "41",
                    "text": "Prolactinomas",
                    "guid": "ECurgBPOaSl_IDHEUEhvGw",
                    "children": []
                  },
                  {
                    "id": "43",
                    "parent": "41",
                    "text": "Macroadenomas",
                    "guid": "RzsJG3t82PYmlxkMGGwQJg",
                    "children": []
                  }
                ]
              }
            ]
          },
          {
            "id": "44",
            "parent": "33",
            "text": "CEFALEA",
            "guid": "JZSfBXT8efgXHBpoA3EMxQ",
            "children": [
              {
                "id": "45",
                "parent": "44",
                "text": "→ Tumores silla turca: cefalea solo si tocan diafragma selar",
                "guid": "DFfHBja3QXcXC4W4CXJGbw",
                "children": []
              },
              {
                "id": "46",
                "parent": "44",
                "text": "→ Tumor pequeño + cefalea = criterio incidentaloma",
                "guid": "SlYSul7FJQwokuz6SJMxMA",
                "children": []
              }
            ]
          }
        ]
      },
      {
        "id": "47",
        "parent": "0",
        "text": "MANEJO",
        "guid": "TYH6RWtg-INik4EHDyjBxg",
        "children": [
          {
            "id": "48",
            "parent": "47",
            "text": "Microadenomas\nasintomáticos:",
            "guid": "Lyen8UyUegto_cJWL1pN7A",
            "children": [
              {
                "id": "49",
                "parent": "48",
                "text": "→ Controversia estudio hormonal",
                "guid": "XaF35BvOSidMBAwlbc0jZQ",
                "children": []
              },
              {
                "id": "50",
                "parent": "48",
                "text": "→ Recomendado estudio inicial",
                "guid": "L5yzUEskPek-FZKAdte8aw",
                "children": []
              },
              {
                "id": "51",
                "parent": "48",
                "text": "→ Seguimiento: <10% crecen (9)",
                "guid": "RqEW4GSsq40PQ8yRbDW15Q",
                "children": []
              }
            ]
          },
          {
            "id": "52",
            "parent": "47",
            "text": "Macroadenomas:",
            "guid": "WaklhSZf5vlvpsKqZgDsiw",
            "children": [
              {
                "id": "53",
                "parent": "52",
                "text": "→ Estudio hormonal imprescindible",
                "guid": "XRcocAaySGJvczL6J207Kw",
                "children": []
              },
              {
                "id": "54",
                "parent": "52",
                "text": "→ Mayor tendencia crecimiento",
                "guid": "ZXdtbhgGH_VwAGxbMvlnsw",
                "children": []
              },
              {
                "id": "55",
                "parent": "52",
                "text": "→ Seguimiento 5 años",
                "guid": "A2cYeFKT7WJCIil6Mo7AaQ",
                "children": []
              }
            ]
          }
        ]
      },
      {
        "id": "56",
        "parent": "0",
        "text": "ALGORITMO\nMANEJO",
        "guid": "HyhnbSsf69Bh6Q5VfMnfUg",
        "children": [
          {
            "id": "57",
            "parent": "56",
            "text": "Hiperfuncionante:",
            "guid": "Ru419y3tGntqlwK3dorpRw",
            "children": [
              {
                "id": "58",
                "parent": "57",
                "text": "→ Prolactinoma: agonistas dopaminérgicos",
                "guid": "eRFYZCislTdtYqWzP7JvRA",
                "children": []
              },
              {
                "id": "59",
                "parent": "57",
                "text": "→ Otros: cirugía",
                "guid": "DVlAxHymckQr6CUpZwJmSQ",
                "children": []
              }
            ]
          },
          {
            "id": "60",
            "parent": "56",
            "text": "No funcionante:",
            "guid": "IwZZPhuO59NNA1LVAB2Brg",
            "children": [
              {
                "id": "61",
                "parent": "60",
                "text": "→ <1cm: RMN (1,3,5 años)",
                "guid": "IkEwNTx2hc8nirzZB7idpA",
                "children": [
                  {
                    "id": "62",
                    "parent": "61",
                    "text": "Sin cambios → No más estudios",
                    "guid": "VHylxBeLKTQ6sgVXV-O-PA",
                    "children": []
                  }
                ]
              },
              {
                "id": "63",
                "parent": "60",
                "text": "→ >1cm:",
                "guid": "ah8Wl3zULtEKcn6mCUd-BA",
                "children": [
                  {
                    "id": "64",
                    "parent": "63",
                    "text": "Campo visual + estudio hipopituitarismo",
                    "guid": "J_QaomxbjPsGEV1WbuJQmQ",
                    "children": []
                  },
                  {
                    "id": "65",
                    "parent": "63",
                    "text": "RMN (0.5,1,2, 3,4,5 años)",
                    "guid": "GkindnCoYA1lbTnhE1n_2w",
                    "children": []
                  },
                  {
                    "id": "66",
                    "parent": "63",
                    "text": "Si tumor crece/campo visual anormal → Cirugía",
                    "guid": "GVT1RVLP35RTDG8fJq42CQ",
                    "children": []
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "67",
        "parent": "0",
        "text": "RECOMENDACIONES\nMANEJO INCIDENTALOMAS",
        "guid": "",
        "children": [
          {
            "id": "68",
            "parent": "67",
            "text": "MICROADENOMAS\nINCIDENTALES",
            "guid": "GoN8HFqz7yhymizFPMSsUg",
            "children": [
              {
                "id": "69",
                "parent": "68",
                "text": "EVALUACIÓN\nHORMONAL INICIAL",
                "guid": "Fyp09xok6Z5EfUn2a6cavA",
                "children": [
                  {
                    "id": "70",
                    "parent": "69",
                    "text": "→ Prolactina",
                    "guid": "MbAS038vT01Ditj4G88pag",
                    "children": []
                  },
                  {
                    "id": "71",
                    "parent": "69",
                    "text": "→ IGF-1",
                    "guid": "fAN-H039V54lFqduI_eYwQ",
                    "children": []
                  },
                  {
                    "id": "72",
                    "parent": "69",
                    "text": "→ Test Nuggent",
                    "guid": "OljkmisoBMUS2elaVKGMEA",
                    "children": []
                  }
                ]
              },
              {
                "id": "73",
                "parent": "68",
                "text": "SEGUIMIENTO\nIMAGEN:",
                "guid": "G9Bk0nhHIztn-4vrNSVaFw",
                "children": [
                  {
                    "id": "74",
                    "parent": "73",
                    "text": "→ 4-10mm: RM anual",
                    "guid": "SxcCzzsH-wtb05AhGo1UqA",
                    "children": []
                  },
                  {
                    "id": "75",
                    "parent": "73",
                    "text": "→ Después: RM c/2 años x5 años total",
                    "guid": "OfyPVGmELHQNCf--VIALcA",
                    "children": []
                  }
                ]
              },
              {
                "id": "76",
                "parent": "68",
                "text": "INDICACIONES\nCIRUGÍA:",
                "guid": "RDgbnH-kLIMRRLfCW2KQkw",
                "children": [
                  {
                    "id": "77",
                    "parent": "76",
                    "text": "→ Adenoma funcionante (excepto prolactinomas)",
                    "guid": "GckWIlXCAbhHCatPS3ko9Q",
                    "children": []
                  },
                  {
                    "id": "78",
                    "parent": "76",
                    "text": "→ Crecimiento + compromiso neurológico/hipopituitarismo",
                    "guid": "VPFRBgqUhEhnSFJfUPTPJQ",
                    "children": []
                  }
                ]
              }
            ]
          },
          {
            "id": "79",
            "parent": "67",
            "text": "MACROADENOMAS\nINCIDENTALES",
            "guid": "WJHb5gxe-c107GfmEurAgA",
            "children": [
              {
                "id": "80",
                "parent": "79",
                "text": "EVALUACIÓN\nINICIAL",
                "guid": "N4b-kgfGUUBnjEyRU1djZQ",
                "children": [
                  {
                    "id": "81",
                    "parent": "80",
                    "text": "a) Hormonal\nbásica:",
                    "guid": "AA10fE-H2HwIfL18SyR3Sw",
                    "children": [
                      {
                        "id": "82",
                        "parent": "81",
                        "text": "→ Prolactina",
                        "guid": "Co_Th2RQTZ1lscvzRIxi2w",
                        "children": []
                      },
                      {
                        "id": "83",
                        "parent": "81",
                        "text": "→ IGF-1",
                        "guid": "TdR6EXK7y7IZDG5MEgyVrQ",
                        "children": []
                      },
                      {
                        "id": "84",
                        "parent": "81",
                        "text": "→ Test Nugent",
                        "guid": "cl_4NSpRJg5tbyZBDCkOVw",
                        "children": []
                      }
                    ]
                  },
                  {
                    "id": "85",
                    "parent": "80",
                    "text": "b) Deficiencias\nhormonales:",
                    "guid": "QvTwilOPuNMwuk2wEMlqnA",
                    "children": [
                      {
                        "id": "86",
                        "parent": "85",
                        "text": "→ Test ACTH-cortisol",
                        "guid": "ABMnxzR40ZBXojdMVQR4zQ",
                        "children": []
                      },
                      {
                        "id": "87",
                        "parent": "85",
                        "text": "→ Hombres: testosterona + LH",
                        "guid": "Pw1V2D7qiasl-UfyF58xvw",
                        "children": []
                      },
                      {
                        "id": "88",
                        "parent": "85",
                        "text": "→ Mujeres: estatus menstrual + estradiol + FSH",
                        "guid": "S0mDeRrlr9gqifI_AtCCCw",
                        "children": []
                      },
                      {
                        "id": "89",
                        "parent": "85",
                        "text": "→ IGF-1",
                        "guid": "IqwBGBIWPtBWJ-VwIrl1lA",
                        "children": []
                      },
                      {
                        "id": "90",
                        "parent": "85",
                        "text": "→ TSH/T4 libre",
                        "guid": "YZ4XTV6kou1t3ezgbC3q1A",
                        "children": []
                      }
                    ]
                  },
                  {
                    "id": "91",
                    "parent": "80",
                    "text": "c) Efecto masa:",
                    "guid": "RL34Gxx2wl9JL4KeA8tN8w",
                    "children": [
                      {
                        "id": "92",
                        "parent": "91",
                        "text": "→ Campimetría Goldman",
                        "guid": "RkuEhUnGu_wi1gBJOKt8uw",
                        "children": []
                      },
                      {
                        "id": "93",
                        "parent": "91",
                        "text": "→ Evaluación neurooftalmológica (pares craneales)",
                        "guid": "dBfiChBFJopE1IsSdCsJ0Q",
                        "children": []
                      }
                    ]
                  }
                ]
              },
              {
                "id": "94",
                "parent": "79",
                "text": "SEGUIMIENTO\nIMAGEN:",
                "guid": "W2FMCm8oypAban-yJqrPgw",
                "children": [
                  {
                    "id": "95",
                    "parent": "94",
                    "text": "→ RM: 6m, 12m, luego anual x5 años",
                    "guid": "Cg56aEX0cfIpe1GPLLp7gQ",
                    "children": []
                  },
                  {
                    "id": "96",
                    "parent": "94",
                    "text": "→ Si borde superior cerca quiasma: RM c/2 años",
                    "guid": "WAqwwn-jNv9Pc_EVOajIDw",
                    "children": []
                  }
                ]
              },
              {
                "id": "97",
                "parent": "79",
                "text": "OBJETIVO\nSEGUIMIENTO:",
                "guid": "IFe8kFTcNVwP2JgJFG-emg",
                "children": [
                  {
                    "id": "98",
                    "parent": "97",
                    "text": "→ Detectar nuevas deficiencias endocrinas",
                    "guid": "XkfZ7D1R3fUl1rLkITzKdw",
                    "children": []
                  },
                  {
                    "id": "99",
                    "parent": "97",
                    "text": "→ Monitorizar crecimiento tumoral",
                    "guid": "EOGWyVaRAJQyBjUTVy0bTg",
                    "children": []
                  }
                ]
              },
              {
                "id": "100",
                "parent": "79",
                "text": "INDICACIONES\nCIRUGÍA:",
                "guid": "DwmTmhyrUph3u-uEZxREXA",
                "children": [
                  {
                    "id": "101",
                    "parent": "100",
                    "text": "→ Adenoma funcionante (excepto prolactinomas)",
                    "guid": "ZSFb51StIxwImqhsKd9UAg",
                    "children": []
                  },
                  {
                    "id": "102",
                    "parent": "100",
                    "text": "→ Compromiso neurológico",
                    "guid": "cSPle1HKKwotqqH1TIUxhQ",
                    "children": []
                  },
                  {
                    "id": "103",
                    "parent": "100",
                    "text": "→ Crecimiento significativo + compromiso neurológico/hipopituitarismo",
                    "guid": "QPL1m0kVIahzMAEJSwFwAw",
                    "children": []
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
