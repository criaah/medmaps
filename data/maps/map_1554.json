    "text": "GINECOMASTIA",
    "guid": "NDNwBFDlzdV7r4PBB4I72g",
    "children": [
      {
        "id": "1",
        "parent": "0",
        "text": "GINECOMASTIA",
        "guid": "SzfyM0AwZGUQau1HPuYP4w",
        "children": []
      },
      {
        "id": "2",
        "parent": "0",
        "text": "└ DEFINICIÓN",
        "guid": "",
        "children": []
      },
      {
        "id": "3",
        "parent": "0",
        "text": "→ Proliferación benigna tej.\nglandular mamario masculino",
        "guid": "",
        "children": []
      },
      {
        "id": "4",
        "parent": "0",
        "text": "→ Diámetro >\n0,5 cm (1)",
        "guid": "",
        "children": []
      },
      {
        "id": "5",
        "parent": "0",
        "text": "└ DIAGNÓSTICO\nDIFERENCIAL",
        "guid": "",
        "children": []
      },
      {
        "id": "6",
        "parent": "0",
        "text": "→\nLipomastia/pseudoginecomastia",
        "guid": "",
        "children": []
      },
      {
        "id": "7",
        "parent": "0",
        "text": "• Depósito grasa\ns/proliferación glandular",
        "guid": "",
        "children": []
      },
      {
        "id": "8",
        "parent": "0",
        "text": "→ Ca. mama\n(infrecuente)",
        "guid": "",
        "children": []
      },
      {
        "id": "9",
        "parent": "0",
        "text": "→ Otras masas: neurofibromas,\nlinfangiomas, hemangiomas,\nlipomas, quistes dermoides",
        "guid": "",
        "children": []
      },
      {
        "id": "10",
        "parent": "0",
        "text": "└ PREVALENCIA\n- 3 períodos:",
        "guid": "",
        "children": []
      },
      {
        "id": "11",
        "parent": "0",
        "text": "1. RN → 60-90% casos",
        "guid": "",
        "children": []
      },
      {
        "id": "12",
        "parent": "0",
        "text": "2. Pubertad →\n70% niños (2,3)",
        "guid": "",
        "children": []
      },
      {
        "id": "13",
        "parent": "0",
        "text": "3. Adultos (50-80a)\n→ 24-65% (4)",
        "guid": "",
        "children": []
      },
      {
        "id": "14",
        "parent": "0",
        "text": "└ PRESENTACIÓN",
        "guid": "",
        "children": []
      },
      {
        "id": "15",
        "parent": "0",
        "text": "→ Bilateral (mayoría)",
        "guid": "",
        "children": []
      },
      {
        "id": "16",
        "parent": "0",
        "text": "→ Asimétrica\n(algunos casos)",
        "guid": "",
        "children": []
      },
      {
        "id": "17",
        "parent": "0",
        "text": "→ Unilateral (posible)",
        "guid": "",
        "children": []
      },
      {
        "id": "18",
        "parent": "0",
        "text": "└ FISIOPATOLOGÍA",
        "guid": "",
        "children": []
      },
      {
        "id": "19",
        "parent": "0",
        "text": "1. Base → Desequilibrio\nestrógenos/andrógenos",
        "guid": "",
        "children": []
      },
      {
        "id": "20",
        "parent": "0",
        "text": "• ↑ estrógenos → hiperplasia ductal +\nproliferación fibroblastos",
        "guid": "",
        "children": []
      },
      {
        "id": "21",
        "parent": "0",
        "text": "• ↑ vascularización",
        "guid": "",
        "children": []
      },
      {
        "id": "22",
        "parent": "0",
        "text": "• Respuesta individual\ns/sensibilidad tej. mamario",
        "guid": "",
        "children": []
      },
      {
        "id": "23",
        "parent": "0",
        "text": "2. Mecanismos:",
        "guid": "",
        "children": []
      },
      {
        "id": "24",
        "parent": "0",
        "text": "→ ↑ producción\nestrógenos",
        "guid": "",
        "children": []
      },
      {
        "id": "25",
        "parent": "0",
        "text": "→ ↓ producción\ntestosterona",
        "guid": "",
        "children": []
      },
      {
        "id": "26",
        "parent": "0",
        "text": "→ ↑ conversión\nandrógenos→estrógenos (tej.\nperiféricos)",
        "guid": "",
        "children": []
      },
      {
        "id": "27",
        "parent": "0",
        "text": "→ ↑ SHBG → ↓\ntestosterona libre",
        "guid": "",
        "children": []
      },
      {
        "id": "28",
        "parent": "0",
        "text": "→ Bloqueo\nreceptor\nandrógenos",
        "guid": "",
        "children": []
      },
      {
        "id": "29",
        "parent": "0",
        "text": "└ CAUSAS\nFISIOLÓGICAS\nPRINCIPALES",
        "guid": "",
        "children": []
      },
      {
        "id": "30",
        "parent": "0",
        "text": "1. Neonatal",
        "guid": "",
        "children": []
      },
      {
        "id": "31",
        "parent": "0",
        "text": "→ Transferencia\nplacentaria estrógenos",
        "guid": "",
        "children": []
      },
      {
        "id": "32",
        "parent": "0",
        "text": "→ Transitoria",
        "guid": "",
        "children": []
      },
      {
        "id": "33",
        "parent": "0",
        "text": "→ Resolución\n~3 semanas",
        "guid": "",
        "children": []
      },
      {
        "id": "34",
        "parent": "0",
        "text": "2. Puberal",
        "guid": "",
        "children": []
      },
      {
        "id": "35",
        "parent": "0",
        "text": "→ Inicio: ↑estrógenos\n> testosterona",
        "guid": "",
        "children": []
      },
      {
        "id": "36",
        "parent": "0",
        "text": "→ Edad: 10-12a",
        "guid": "",
        "children": []
      },
      {
        "id": "37",
        "parent": "0",
        "text": "→ Pico: 13-14a\n(Tanner 3)",
        "guid": "",
        "children": []
      },
      {
        "id": "38",
        "parent": "0",
        "text": "→ Regresión\n≤24m (antes 17a)",
        "guid": "",
        "children": []
      },
      {
        "id": "39",
        "parent": "0",
        "text": "└ CAUSAS\nFISIOLÓGICAS\n(cont.)",
        "guid": "ZOpSERGnTn55H3LcJacLNg",
        "children": []
      },
      {
        "id": "40",
        "parent": "0",
        "text": "3. Adulto (>50a)",
        "guid": "",
        "children": []
      },
      {
        "id": "41",
        "parent": "0",
        "text": "→ ↓ testosterona",
        "guid": "",
        "children": []
      },
      {
        "id": "42",
        "parent": "0",
        "text": "→ ↑ conversión periférica\nandrógenos→estrógenos",
        "guid": "",
        "children": []
      },
      {
        "id": "43",
        "parent": "0",
        "text": "→ ↑ tejido\nadiposo c/edad",
        "guid": "",
        "children": []
      },
      {
        "id": "44",
        "parent": "0",
        "text": "└ ETIOLOGÍA\nADULTO (1)",
        "guid": "",
        "children": []
      },
      {
        "id": "45",
        "parent": "0",
        "text": "→ Persistencia\nginecomastia puberal:\n25%",
        "guid": "",
        "children": []
      },
      {
        "id": "46",
        "parent": "0",
        "text": "→ Drogas: 10-25%",
        "guid": "",
        "children": []
      },
      {
        "id": "47",
        "parent": "0",
        "text": "→ Idiopática: 25-30%",
        "guid": "",
        "children": []
      },
      {
        "id": "48",
        "parent": "0",
        "text": "→\nCirrosis/malnutrición:\n8%",
        "guid": "",
        "children": []
      },
      {
        "id": "49",
        "parent": "0",
        "text": "→ Hipogonadismo: 10%",
        "guid": "",
        "children": []
      },
      {
        "id": "50",
        "parent": "0",
        "text": "→ Tumores\ntesticulares: 3%",
        "guid": "",
        "children": []
      },
      {
        "id": "51",
        "parent": "0",
        "text": "→ Hipertiroidismo: 1.5%",
        "guid": "",
        "children": []
      },
      {
        "id": "52",
        "parent": "0",
        "text": "→ Insuf. renal\ncrónica: 1%",
        "guid": "",
        "children": []
      },
      {
        "id": "53",
        "parent": "0",
        "text": "└ DIAGNÓSTICO",
        "guid": "",
        "children": []
      },
      {
        "id": "54",
        "parent": "0",
        "text": "1. Examen físico",
        "guid": "",
        "children": []
      },
      {
        "id": "55",
        "parent": "0",
        "text": "→ Palpar mama:\nbotón mamario",
        "guid": "",
        "children": []
      },
      {
        "id": "56",
        "parent": "0",
        "text": "→ Características:",
        "guid": "",
        "children": []
      },
      {
        "id": "57",
        "parent": "0",
        "text": "• Concéntrico c/pezón",
        "guid": "",
        "children": []
      },
      {
        "id": "58",
        "parent": "0",
        "text": "• Consistencia\nelástica/firme",
        "guid": "",
        "children": []
      },
      {
        "id": "59",
        "parent": "0",
        "text": "• Bilateral 50% casos",
        "guid": "",
        "children": []
      },
      {
        "id": "60",
        "parent": "0",
        "text": "→ Medir diámetro",
        "guid": "",
        "children": []
      },
      {
        "id": "61",
        "parent": "0",
        "text": "→ Evaluar sensibilidad\n(<6m: posible dolor)",
        "guid": "",
        "children": []
      },
      {
        "id": "62",
        "parent": "0",
        "text": "└ FÁRMACOS\nCAUSANTES",
        "guid": "",
        "children": []
      },
      {
        "id": "63",
        "parent": "0",
        "text": "1. Antiandrógenos: flutamida,\nbicalutamida, nilutamida, finasteride,\ndutasteride, espironolactona",
        "guid": "",
        "children": []
      },
      {
        "id": "64",
        "parent": "0",
        "text": "2. Antimicóticos: isoniacida,\nketoconazol, metronidazol",
        "guid": "",
        "children": []
      },
      {
        "id": "65",
        "parent": "0",
        "text": "3. Antiulcerosos:\nranitidina, omeprazol",
        "guid": "",
        "children": []
      },
      {
        "id": "66",
        "parent": "0",
        "text": "4. Quimioterapia: agentes\nalquilantes, metotrexato",
        "guid": "",
        "children": []
      },
      {
        "id": "67",
        "parent": "0",
        "text": "5. Cardiovasculares: enalapril,\namiodarona, digoxina, bloq.Ca,\ndiltiazem, nifedipino",
        "guid": "",
        "children": []
      },
      {
        "id": "68",
        "parent": "0",
        "text": "6. Drogas abuso: alcohol, heroína,\nanfetaminas, marihuana",
        "guid": "",
        "children": []
      },
      {
        "id": "69",
        "parent": "0",
        "text": "7. Hormonas: andrógenos,\nesteroides, estrógenos",
        "guid": "",
        "children": []
      },
      {
        "id": "70",
        "parent": "0",
        "text": "8. Psicofármacos: fenotiazinas,\nhaloperidol, antidepresivos tricíclicos",
        "guid": "",
        "children": []
      },
      {
        "id": "71",
        "parent": "0",
        "text": "9. Otros: terapia HAART, teofilina,\nmetoclopramida, domperidona",
        "guid": "",
        "children": []
      },
      {
        "id": "72",
        "parent": "0",
        "text": "└ DIAGNÓSTICO DIFERENCIAL CA.\nMAMA vs GINECOMASTIA",
        "guid": "f4EcW1QxhP9WQo6LV03YlQ",
        "children": []
      },
      {
        "id": "73",
        "parent": "0",
        "text": "1. Carcinoma",
        "guid": "",
        "children": []
      },
      {
        "id": "74",
        "parent": "0",
        "text": "→ Duro/firme",
        "guid": "",
        "children": []
      },
      {
        "id": "75",
        "parent": "0",
        "text": "→ Fuera complejo\nareola-pezón",
        "guid": "",
        "children": []
      },
      {
        "id": "76",
        "parent": "0",
        "text": "→ Unilateral",
        "guid": "",
        "children": []
      },
      {
        "id": "77",
        "parent": "0",
        "text": "→ Posible\nretracción\npezón",
        "guid": "",
        "children": []
      },
      {
        "id": "78",
        "parent": "0",
        "text": "→\nDescarga/sangrado\n(10%)",
        "guid": "",
        "children": []
      },
      {
        "id": "79",
        "parent": "0",
        "text": "2. Ginecomastia",
        "guid": "",
        "children": []
      },
      {
        "id": "80",
        "parent": "0",
        "text": "→ Masa cauchos/firme",
        "guid": "",
        "children": []
      },
      {
        "id": "81",
        "parent": "0",
        "text": "→ Concéntrica c/pezón",
        "guid": "",
        "children": []
      },
      {
        "id": "82",
        "parent": "0",
        "text": "→ Bilateral común",
        "guid": "",
        "children": []
      },
      {
        "id": "83",
        "parent": "0",
        "text": "→ No retracción",
        "guid": "",
        "children": []
      },
      {
        "id": "84",
        "parent": "0",
        "text": "→ No descarga usual",
        "guid": "",
        "children": []
      },
      {
        "id": "85",
        "parent": "0",
        "text": "└ EVALUACIÓN CLÍNICA -\nRECOMENDACIONES",
        "guid": "",
        "children": []
      },
      {
        "id": "86",
        "parent": "0",
        "text": "→ Historia clínica:",
        "guid": "",
        "children": []
      },
      {
        "id": "87",
        "parent": "0",
        "text": "• Edad aparición",
        "guid": "",
        "children": []
      },
      {
        "id": "88",
        "parent": "0",
        "text": "•\nEtapa/evolución\npuberal",
        "guid": "",
        "children": []
      },
      {
        "id": "89",
        "parent": "0",
        "text": "• Duración",
        "guid": "",
        "children": []
      },
      {
        "id": "90",
        "parent": "0",
        "text": "• Síntomas\n(dolor, secreción)",
        "guid": "",
        "children": []
      },
      {
        "id": "91",
        "parent": "0",
        "text": "└\nEXPLORACIÓN\nFÍSICA",
        "guid": "",
        "children": []
      },
      {
        "id": "92",
        "parent": "0",
        "text": "→ Posición:\ndecúbito dorsal",
        "guid": "",
        "children": []
      },
      {
        "id": "93",
        "parent": "0",
        "text": "→ Técnica:",
        "guid": "",
        "children": []
      },
      {
        "id": "94",
        "parent": "0",
        "text": "• Manos detrás cabeza",
        "guid": "",
        "children": []
      },
      {
        "id": "95",
        "parent": "0",
        "text": "• Examinar\nc/dedos juntos",
        "guid": "",
        "children": []
      },
      {
        "id": "96",
        "parent": "0",
        "text": "• Palpar tej. subcutáneo",
        "guid": "",
        "children": []
      },
      {
        "id": "97",
        "parent": "0",
        "text": "• Buscar:",
        "guid": "",
        "children": []
      },
      {
        "id": "98",
        "parent": "0",
        "text": "- Ginecomastia:\nmasa concéntrica",
        "guid": "",
        "children": []
      },
      {
        "id": "99",
        "parent": "0",
        "text": "- Pseudoginecomastia:\nausencia botón",
        "guid": "",
        "children": []
      },
      {
        "id": "100",
        "parent": "0",
        "text": "• Evaluar área\nmamaria completa",
        "guid": "",
        "children": []
      },
      {
        "id": "101",
        "parent": "0",
        "text": "• Descartar adenopatías",
        "guid": "",
        "children": []
      },
      {
        "id": "102",
        "parent": "0",
        "text": "└ EVALUACIÓN\nCLÍNICA (cont.)",
        "guid": "CSBfTzqLZq8SbbAgN0SA8g",
        "children": []
      },
      {
        "id": "103",
        "parent": "0",
        "text": "→ Historia\nclínica adicional:",
        "guid": "",
        "children": []
      },
      {
        "id": "104",
        "parent": "0",
        "text": "• Estado nutricional",
        "guid": "",
        "children": []
      },
      {
        "id": "105",
        "parent": "0",
        "text": "• Cambios\npeso recientes",
        "guid": "",
        "children": []
      },
      {
        "id": "106",
        "parent": "0",
        "text": "• Potencia sexual/libido",
        "guid": "",
        "children": []
      },
      {
        "id": "107",
        "parent": "0",
        "text": "• Anamnesis\ndirigida (tabla 21-1)",
        "guid": "",
        "children": []
      },
      {
        "id": "108",
        "parent": "0",
        "text": "• Fármacos\nasociados (tabla\n21-2)",
        "guid": "",
        "children": []
      },
      {
        "id": "109",
        "parent": "0",
        "text": "└ EFICACIA\nDIAGNÓSTICA",
        "guid": "",
        "children": []
      },
      {
        "id": "110",
        "parent": "0",
        "text": "→ HC + Ex. físico →\n30-40% casos",
        "guid": "",
        "children": []
      },
      {
        "id": "111",
        "parent": "0",
        "text": "→ Examen testicular\n→ obligatorio",
        "guid": "",
        "children": []
      },
      {
        "id": "112",
        "parent": "0",
        "text": "└ EVALUACIÓN\nBIOQUÍMICA -\nINDICACIONES:",
        "guid": "",
        "children": []
      },
      {
        "id": "113",
        "parent": "0",
        "text": "1. Desarrollo\nrápido (>20a)",
        "guid": "",
        "children": []
      },
      {
        "id": "114",
        "parent": "0",
        "text": "2. Sensibilidad mamaria",
        "guid": "",
        "children": []
      },
      {
        "id": "115",
        "parent": "0",
        "text": "3. Masas\nexcéntricas/firmes",
        "guid": "",
        "children": []
      },
      {
        "id": "116",
        "parent": "0",
        "text": "4. Ginecomastia >4cm",
        "guid": "",
        "children": []
      },
      {
        "id": "117",
        "parent": "0",
        "text": "5. Edad <10a",
        "guid": "",
        "children": []
      },
      {
        "id": "118",
        "parent": "0",
        "text": "└ ESTUDIOS LAB",
        "guid": "",
        "children": []
      },
      {
        "id": "119",
        "parent": "0",
        "text": "1. Ginecomastia\nasintomática crónica:",
        "guid": "",
        "children": []
      },
      {
        "id": "120",
        "parent": "0",
        "text": "→ Ex. físico normal →\nbajo rendimiento lab",
        "guid": "",
        "children": []
      },
      {
        "id": "121",
        "parent": "0",
        "text": "→ Recomendado:",
        "guid": "",
        "children": []
      },
      {
        "id": "122",
        "parent": "0",
        "text": "• Testosterona total",
        "guid": "",
        "children": []
      },
      {
        "id": "123",
        "parent": "0",
        "text": "• Biodisponible",
        "guid": "",
        "children": []
      },
      {
        "id": "124",
        "parent": "0",
        "text": "• LH (AM, <10h)",
        "guid": "",
        "children": []
      },
      {
        "id": "125",
        "parent": "0",
        "text": "• TSH (screening\nhipertiroidismo)",
        "guid": "",
        "children": []
      },
      {
        "id": "126",
        "parent": "0",
        "text": "2. Ex. físico anormal:",
        "guid": "",
        "children": []
      },
      {
        "id": "127",
        "parent": "0",
        "text": "→ Lab orientado\ns/sospecha diagnóstica",
        "guid": "",
        "children": []
      },
      {
        "id": "128",
        "parent": "0",
        "text": "└\nCONSIDERACIONES\nhCG",
        "guid": "",
        "children": []
      },
      {
        "id": "129",
        "parent": "0",
        "text": "→ ↑ 18% neoplasias\nno trofoblásticas",
        "guid": "",
        "children": []
      },
      {
        "id": "130",
        "parent": "0",
        "text": "→ ↑ tumor\ncélulas\ngerminales",
        "guid": "",
        "children": []
      },
      {
        "id": "131",
        "parent": "0",
        "text": "→ No indicada rutina\ns/sospecha clínica",
        "guid": "",
        "children": []
      },
      {
        "id": "132",
        "parent": "0",
        "text": "└ EVALUACIÓN\nRADIOLÓGICA",
        "guid": "",
        "children": []
      },
      {
        "id": "133",
        "parent": "0",
        "text": "1. Palpación\ntesticular anormal:",
        "guid": "",
        "children": []
      },
      {
        "id": "134",
        "parent": "0",
        "text": "→ Ecografía bilateral",
        "guid": "",
        "children": []
      },
      {
        "id": "135",
        "parent": "0",
        "text": "2. Mamografía:",
        "guid": "",
        "children": []
      },
      {
        "id": "136",
        "parent": "0",
        "text": "→ Objetivo: diferencial\nCa vs ginecomastia",
        "guid": "",
        "children": []
      },
      {
        "id": "137",
        "parent": "0",
        "text": "→\nSensibilidad/especificidad:\n90% (6)",
        "guid": "",
        "children": []
      },
      {
        "id": "138",
        "parent": "0",
        "text": "→ Factores riesgo Ca:",
        "guid": "",
        "children": []
      },
      {
        "id": "139",
        "parent": "0",
        "text": "• Sd. Klinefelter (3-6%)",
        "guid": "",
        "children": []
      },
      {
        "id": "140",
        "parent": "0",
        "text": "• Familiar Ca\nmama joven",
        "guid": "",
        "children": []
      },
      {
        "id": "141",
        "parent": "0",
        "text": "└ PATOLOGÍAS ASOCIADAS:\nSÍNTOMAS Y SIGNOS",
        "guid": "fypla0ORlih_pL6hT8SXuw",
        "children": []
      },
      {
        "id": "142",
        "parent": "0",
        "text": "1. TUMORES",
        "guid": "",
        "children": []
      },
      {
        "id": "143",
        "parent": "0",
        "text": "→ Testicular Leydig",
        "guid": "",
        "children": []
      },
      {
        "id": "144",
        "parent": "0",
        "text": "• Sínt: dolor, ↓crecimiento\ntesticular, ↓libido",
        "guid": "",
        "children": []
      },
      {
        "id": "145",
        "parent": "0",
        "text": "• Sign: masa testicular, atrofia\ncontralateral, feminización",
        "guid": "",
        "children": []
      },
      {
        "id": "146",
        "parent": "0",
        "text": "→ Testicular germinal",
        "guid": "",
        "children": []
      },
      {
        "id": "147",
        "parent": "0",
        "text": "• Sínt: dolor,\n↓crecimiento, sínt.\nmetástasis",
        "guid": "",
        "children": []
      },
      {
        "id": "148",
        "parent": "0",
        "text": "• Sign: masa testicular,\nadenopatías, hemoptisis",
        "guid": "",
        "children": []
      },
      {
        "id": "149",
        "parent": "0",
        "text": "→ Carcinoma\nsuprarrenal",
        "guid": "",
        "children": []
      },
      {
        "id": "150",
        "parent": "0",
        "text": "• Sínt: ↓peso,\n↓libido, Sd. Cushing",
        "guid": "",
        "children": []
      },
      {
        "id": "151",
        "parent": "0",
        "text": "• Sign: masa abdominal, exceso\nglucocorticoides/mineralocorticoides",
        "guid": "",
        "children": []
      },
      {
        "id": "152",
        "parent": "0",
        "text": "2. HIPOGONADISMO",
        "guid": "",
        "children": []
      },
      {
        "id": "153",
        "parent": "0",
        "text": "→ Primario",
        "guid": "",
        "children": []
      },
      {
        "id": "154",
        "parent": "0",
        "text": "• Sínt: ↓libido, disfunción\neréctil, sínt. vasomotores",
        "guid": "",
        "children": []
      },
      {
        "id": "155",
        "parent": "0",
        "text": "• Sign: ↓tamaño testicular, Sd.\nKlinefelter, caracteres sexuales\n2os incompletos",
        "guid": "",
        "children": []
      },
      {
        "id": "156",
        "parent": "0",
        "text": "→ Secundario",
        "guid": "",
        "children": []
      },
      {
        "id": "157",
        "parent": "0",
        "text": "• Sínt: ↓libido, disfunción eréctil,\ndéficit hormonal hipofisario",
        "guid": "",
        "children": []
      },
      {
        "id": "158",
        "parent": "0",
        "text": "• Sign: ↓tamaño testicular,\nalt. campo visual,\ntumor/parásito/hipotiroidismo",
        "guid": "",
        "children": []
      },
      {
        "id": "159",
        "parent": "0",
        "text": "3. OTRAS CAUSAS",
        "guid": "",
        "children": []
      },
      {
        "id": "160",
        "parent": "0",
        "text": "→ Hipertiroidismo",
        "guid": "",
        "children": []
      },
      {
        "id": "161",
        "parent": "0",
        "text": "• Sínt: ↓peso, palpitaciones,\ndiaforesis, intolerancia calor",
        "guid": "",
        "children": []
      },
      {
        "id": "162",
        "parent": "0",
        "text": "• Sign: bocio, temblor,\ntaquicardia, retracción palpebral",
        "guid": "",
        "children": []
      },
      {
        "id": "163",
        "parent": "0",
        "text": "→ Enf. hepática",
        "guid": "",
        "children": []
      },
      {
        "id": "164",
        "parent": "0",
        "text": "• Sínt: anorexia, náuseas, vómitos,\n↓peso, edema, ictericia, prurito",
        "guid": "",
        "children": []
      },
      {
        "id": "165",
        "parent": "0",
        "text": "• Sign: hígado\ngrande/atrófico,\nascitis",
        "guid": "",
        "children": []
      },
      {
        "id": "166",
        "parent": "0",
        "text": "→ Enf. renal",
        "guid": "",
        "children": []
      },
      {
        "id": "167",
        "parent": "0",
        "text": "• Sínt: anorexia, fatiga,\nnáuseas, vómitos, oliguria",
        "guid": "",
        "children": []
      },
      {
        "id": "168",
        "parent": "0",
        "text": "• Sign: letargia, palidez, asterixis,\nhalitosis urémica, HTA",
        "guid": "",
        "children": []
      },
      {
        "id": "169",
        "parent": "0",
        "text": "→ Insensibilidad\nandrógenos",
        "guid": "",
        "children": []
      },
      {
        "id": "170",
        "parent": "0",
        "text": "• Sínt: ↓libido, infertilidad",
        "guid": "",
        "children": []
      },
      {
        "id": "171",
        "parent": "0",
        "text": "• Sign: hipospadias, genitales\nambiguos, debilidad muscular",
        "guid": "",
        "children": []
      },
      {
        "id": "172",
        "parent": "0",
        "text": "→ Sd. familiar\nexceso aromatasa",
        "guid": "",
        "children": []
      },
      {
        "id": "173",
        "parent": "0",
        "text": "• Sínt: ninguno",
        "guid": "",
        "children": []
      },
      {
        "id": "174",
        "parent": "0",
        "text": "• Sign: ginecomastia prepuberal, ↑estatura\nniñez, virilización incompleta",
        "guid": "",
        "children": []
      },
      {
        "id": "175",
        "parent": "0",
        "text": "└ ESTUDIOS IMAGEN\nADICIONALES",
        "guid": "",
        "children": []
      },
      {
        "id": "176",
        "parent": "0",
        "text": "→ Ecografía partes blandas: diferencial\nginecomastia vs lipomastia",
        "guid": "",
        "children": []
      },
      {
        "id": "177",
        "parent": "0",
        "text": "→ Mamografía: obligatoria en\nginecomastia excéntrica (descartar Ca)",
        "guid": "",
        "children": []
      },
      {
        "id": "178",
        "parent": "0",
        "text": "└ ALGORITMO MANEJO\nGINECOMASTIA (10)",
        "guid": "ccn4AlY5GjcoLSySQll9tg",
        "children": []
      },
      {
        "id": "179",
        "parent": "0",
        "text": "1. PACIENTE →\n↑volumen mamario",
        "guid": "",
        "children": []
      },
      {
        "id": "180",
        "parent": "0",
        "text": "└→ Diferencial:",
        "guid": "",
        "children": []
      },
      {
        "id": "181",
        "parent": "0",
        "text": "• Ginecomastia\nverdadera",
        "guid": "",
        "children": []
      },
      {
        "id": "182",
        "parent": "0",
        "text": "• Pseudoginecomastia →\nobservar/evaluar cirugía",
        "guid": "",
        "children": []
      },
      {
        "id": "183",
        "parent": "0",
        "text": "2. GINECOMASTIA\nVERDADERA",
        "guid": "",
        "children": []
      },
      {
        "id": "184",
        "parent": "0",
        "text": "└→ Clasificar:",
        "guid": "",
        "children": []
      },
      {
        "id": "185",
        "parent": "0",
        "text": "• Concéntrica",
        "guid": "",
        "children": []
      },
      {
        "id": "186",
        "parent": "0",
        "text": "• Excéntrica →\nmamografía (descartar\nCa)",
        "guid": "",
        "children": []
      },
      {
        "id": "187",
        "parent": "0",
        "text": "3. GINECOMASTIA\nCONCÉNTRICA",
        "guid": "",
        "children": []
      },
      {
        "id": "188",
        "parent": "0",
        "text": "└→ Evaluar\ntiempo evolución:",
        "guid": "",
        "children": []
      },
      {
        "id": "189",
        "parent": "0",
        "text": "A. Reciente/sintomática:",
        "guid": "",
        "children": []
      },
      {
        "id": "190",
        "parent": "0",
        "text": "→ Descartar causas",
        "guid": "",
        "children": []
      },
      {
        "id": "191",
        "parent": "0",
        "text": "→ Evaluar uso\ntamoxifeno",
        "guid": "",
        "children": []
      },
      {
        "id": "192",
        "parent": "0",
        "text": "→ Si falla tto\nmédico → cirugía",
        "guid": "",
        "children": []
      },
      {
        "id": "193",
        "parent": "0",
        "text": "B. >1 año\n(asintomática):",
        "guid": "",
        "children": []
      },
      {
        "id": "194",
        "parent": "0",
        "text": "→ Descartar causas",
        "guid": "",
        "children": []
      },
      {
        "id": "195",
        "parent": "0",
        "text": "→ Evaluar cirugía",
        "guid": "",
        "children": []
      },
      {
        "id": "196",
        "parent": "0",
        "text": "└ TRATAMIENTO -\nINDICACIONES",
        "guid": "",
        "children": []
      },
      {
        "id": "197",
        "parent": "0",
        "text": "1. Criterios inicio:",
        "guid": "",
        "children": []
      },
      {
        "id": "198",
        "parent": "0",
        "text": "→ Causa identificada",
        "guid": "",
        "children": []
      },
      {
        "id": "199",
        "parent": "0",
        "text": "→ Precoz (<6m inicio)",
        "guid": "",
        "children": []
      },
      {
        "id": "200",
        "parent": "0",
        "text": "→ Fase\nproliferativa\ndolorosa",
        "guid": "",
        "children": []
      },
      {
        "id": "201",
        "parent": "0",
        "text": "2. Pronóstico:",
        "guid": "",
        "children": []
      },
      {
        "id": "202",
        "parent": "0",
        "text": "→ Casos crónicos (>1a):\nfibrosis → dificulta regresión",
        "guid": "",
        "children": []
      },
      {
        "id": "203",
        "parent": "0",
        "text": "→ Posible regresión:",
        "guid": "",
        "children": []
      },
      {
        "id": "204",
        "parent": "0",
        "text": "• Suspensión\nfármaco causante",
        "guid": "",
        "children": []
      },
      {
        "id": "205",
        "parent": "0",
        "text": "• Inicio testosterona en\nhipogonadismo",
        "guid": "",
        "children": []
      },
      {
        "id": "206",
        "parent": "0",
        "text": "3. Farmacoterapia:",
        "guid": "",
        "children": []
      },
      {
        "id": "207",
        "parent": "0",
        "text": "→ Mayoría ttos:\nescasa evidencia",
        "guid": "",
        "children": []
      },
      {
        "id": "208",
        "parent": "0",
        "text": "→ Estudios:\nretrospectivos/aleatorizados\nlimitados",
        "guid": "",
        "children": []
      },
      {
        "id": "209",
        "parent": "0",
        "text": "→ Opciones\n(resultados\nvariables):",
        "guid": "",
        "children": []
      },
      {
        "id": "210",
        "parent": "0",
        "text": "• Tamoxifeno",
        "guid": "",
        "children": []
      },
      {
        "id": "211",
        "parent": "0",
        "text": "• Raloxifeno",
        "guid": "",
        "children": []
      },
      {
        "id": "212",
        "parent": "0",
        "text": "• Anastrozol\n(inhibidor\naromatasa)",
        "guid": "",
        "children": []
      },
      {
        "id": "213",
        "parent": "0",
        "text": "4. Tamoxifeno:",
        "guid": "",
        "children": []
      },
      {
        "id": "214",
        "parent": "0",
        "text": "→ No aprobado FDA",
        "guid": "",
        "children": []
      },
      {
        "id": "215",
        "parent": "0",
        "text": "→ Dosis:\n10mg/12h x\n3-6m",
        "guid": "",
        "children": []
      },
      {
        "id": "216",
        "parent": "0",
        "text": "→ Indicación: ginecomastia <3cm,\nreciente, proliferativa/dolorosa",
        "guid": "",
        "children": []
      },
      {
        "id": "217",
        "parent": "0",
        "text": "→ Respuesta (1m):",
        "guid": "",
        "children": []
      },
      {
        "id": "218",
        "parent": "0",
        "text": "• ↓dolor",
        "guid": "",
        "children": []
      },
      {
        "id": "219",
        "parent": "0",
        "text": "• ↓sensibilidad",
        "guid": "",
        "children": []
      },
      {
        "id": "220",
        "parent": "0",
        "text": "• ↓tamaño posterior",
        "guid": "",
        "children": []
      },
      {
        "id": "221",
        "parent": "0",
        "text": "└ TAMOXIFENO (cont.)",
        "guid": "ApeMEA2T8JdVRZAeNsr8FA",
        "children": []
      },
      {
        "id": "222",
        "parent": "0",
        "text": "→ Eficacia:",
        "guid": "",
        "children": []
      },
      {
        "id": "223",
        "parent": "0",
        "text": "• Resolución\ncompleta: 78% casos\n(7)",
        "guid": "",
        "children": []
      },
      {
        "id": "224",
        "parent": "0",
        "text": "• Efectos\nadversos\n(n=225):",
        "guid": "",
        "children": []
      },
      {
        "id": "225",
        "parent": "0",
        "text": "- Molestias\nabdominales (2\npac.)",
        "guid": "",
        "children": []
      },
      {
        "id": "226",
        "parent": "0",
        "text": "- TVP\npostraumática (1\npac.) (8)",
        "guid": "",
        "children": []
      },
      {
        "id": "227",
        "parent": "0",
        "text": "• Prevención en\nQT próstata:",
        "guid": "",
        "children": []
      },
      {
        "id": "228",
        "parent": "0",
        "text": "- Grupo tamoxifeno:\n10% ginecomastia",
        "guid": "",
        "children": []
      },
      {
        "id": "229",
        "parent": "0",
        "text": "- Grupo control: 73%\nginecomastia (9)",
        "guid": "",
        "children": []
      },
      {
        "id": "230",
        "parent": "0",
        "text": "└ GINECOMASTIA\nNEONATAL/PUBERAL",
        "guid": "",
        "children": []
      },
      {
        "id": "231",
        "parent": "0",
        "text": "→ Alta tasa\nregresión\nespontánea",
        "guid": "",
        "children": []
      },
      {
        "id": "232",
        "parent": "0",
        "text": "→ Manejo: observación si\nclínicamente sanos",
        "guid": "",
        "children": []
      },
      {
        "id": "233",
        "parent": "0",
        "text": "└ INDICACIONES\nQUIRÚRGICAS",
        "guid": "",
        "children": []
      },
      {
        "id": "234",
        "parent": "0",
        "text": "1. Criterios:",
        "guid": "",
        "children": []
      },
      {
        "id": "235",
        "parent": "0",
        "text": "→ No respuesta a:",
        "guid": "",
        "children": []
      },
      {
        "id": "236",
        "parent": "0",
        "text": "• Suspensión fármacos",
        "guid": "",
        "children": []
      },
      {
        "id": "237",
        "parent": "0",
        "text": "• Corrección enf.\nsubyacente",
        "guid": "",
        "children": []
      },
      {
        "id": "238",
        "parent": "0",
        "text": "• Tamoxifeno",
        "guid": "",
        "children": []
      },
      {
        "id": "239",
        "parent": "0",
        "text": "2. Tipos:",
        "guid": "",
        "children": []
      },
      {
        "id": "240",
        "parent": "0",
        "text": "→ Evolución prolongada",
        "guid": "",
        "children": []
      },
      {
        "id": "241",
        "parent": "0",
        "text": "→ Gran tamaño",
        "guid": "",
        "children": []
      },
      {
        "id": "242",
        "parent": "0",
        "text": "→ Refractarias",
        "guid": "",
        "children": []
      },
      {
        "id": "243",
        "parent": "0",
        "text": "→ Repercusión\npsicológica",
        "guid": "",
        "children": []
      },
      {
        "id": "244",
        "parent": "0",
        "text": "→ Apariencia\ninsatisfactoria",
        "guid": "",
        "children": []
      },
      {
        "id": "245",
        "parent": "0",
        "text": "3. Técnica:",
        "guid": "",
        "children": []
      },
      {
        "id": "246",
        "parent": "0",
        "text": "→ Remoción\nmínimamente\ninvasiva:",
        "guid": "",
        "children": []
      },
      {
        "id": "247",
        "parent": "0",
        "text": "• Glándula",
        "guid": "",
        "children": []
      },
      {
        "id": "248",
        "parent": "0",
        "text": "• Tejido graso\nsubareolar",
        "guid": "",
        "children": []
      },
      {
        "id": "249",
        "parent": "0",
        "text": "→ Satisfacción\nposquirúrgica: 60%\n(11)",
        "guid": "",
        "children": []
      },
      {
        "id": "250",
        "parent": "0",
        "text": "4. Observación:",
        "guid": "",
        "children": []
      },
      {
        "id": "251",
        "parent": "0",
        "text": "→ Indicada: ginecomastia crónica\nmoderada asintomática",
        "guid": "",
        "children": []
      }
