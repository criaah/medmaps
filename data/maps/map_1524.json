    "text": "MANEJO ENDOCRINOLÓGICO\nPOSTCIRUGÍA HIPOFISIARIA",
    "guid": "aM8vHlwBcK9EpiLnYKTpJQ",
    "children": [
      {
        "id": "1",
        "parent": "0",
        "text": "MONITORIZACIÓN\nPOSTQX",
        "guid": "",
        "children": [
          {
            "id": "2",
            "parent": "1",
            "text": "→ Vía transesfenoidal: mayoría sin complicaciones",
            "guid": "YJuAtXF1DXVw2ZXwJXdhfQ",
            "children": []
          },
          {
            "id": "3",
            "parent": "1",
            "text": "→ Protocolo obligatorio: detectar disfunción hipófisis ant/post",
            "guid": "Es1SDF2o9EpoIfTCIqGbxw",
            "children": []
          }
        ]
      },
      {
        "id": "4",
        "parent": "0",
        "text": "ESTADOS\nPOLIÚRICOS",
        "guid": "",
        "children": [
          {
            "id": "5",
            "parent": "4",
            "text": "Poliuria hipotónica:",
            "guid": "OiJX0T483VJQ6aHAGdnHYw",
            "children": [
              {
                "id": "6",
                "parent": "5",
                "text": "→ Criterios:",
                "guid": "P2jhsxAu-ZRanDvqSwgiKw",
                "children": [
                  {
                    "id": "7",
                    "parent": "6",
                    "text": "Diuresis >3000 cm³/día",
                    "guid": "MXN-VDgkuXdeyXtRK6rXLQ",
                    "children": []
                  },
                  {
                    "id": "8",
                    "parent": "6",
                    "text": "Densidad <1.010",
                    "guid": "GKqfRhZ7U8lSsUZnAXnOZA",
                    "children": []
                  }
                ]
              },
              {
                "id": "9",
                "parent": "5",
                "text": "→ Frecuencia:",
                "guid": "cnzEeBdXaU9iHreKEiZRCA",
                "children": [
                  {
                    "id": "10",
                    "parent": "9",
                    "text": "Transitoria: 18-31% casos (1)",
                    "guid": "Xzc-iXu0eeFnKvjcP9K_Pg",
                    "children": []
                  },
                  {
                    "id": "11",
                    "parent": "9",
                    "text": "DI permanente: 1.4-3. 4% casos (1,2)",
                    "guid": "bSmHV1gEjsxlSiC7f_bZYw",
                    "children": []
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "12",
        "parent": "0",
        "text": "EVOLUCIÓN DI",
        "guid": "",
        "children": [
          {
            "id": "13",
            "parent": "12",
            "text": "→ Patrones:",
            "guid": "Na2DFk1sFX0imHUqb8_a6A",
            "children": [
              {
                "id": "14",
                "parent": "13",
                "text": "a) DI simple",
                "guid": "C6jy0HOCFuoJqaJLSxHUgw",
                "children": []
              },
              {
                "id": "15",
                "parent": "13",
                "text": "b) SSIADH",
                "guid": "A7EQf2RF3jYWGfavNSSO0w",
                "children": []
              },
              {
                "id": "16",
                "parent": "13",
                "text": "c) Patrón trifásico\n(DI-SSIADH-DI) (1,3)",
                "guid": "HGqXrXTjcgBgz2YBNRU28w",
                "children": []
              }
            ]
          }
        ]
      },
      {
        "id": "17",
        "parent": "0",
        "text": "DIAGNÓSTICO\nDIFERENCIAL POLIURIA",
        "guid": "C17FyTOArGg2jwVXfduKQQ",
        "children": [
          {
            "id": "18",
            "parent": "17",
            "text": "Sin DI presente:",
            "guid": "StgVtxitvOEQAdtJKg9UQQ",
            "children": [
              {
                "id": "19",
                "parent": "18",
                "text": "→ Descartar:",
                "guid": "FGI2w3cs1CVp4hN_AYu-Gg",
                "children": [
                  {
                    "id": "20",
                    "parent": "19",
                    "text": "Hiperglicemia (DM/corticoides periop)",
                    "guid": "TzFi8U8sNDsBgpd9BN7mCA",
                    "children": []
                  },
                  {
                    "id": "21",
                    "parent": "19",
                    "text": "Exceso volumen",
                    "guid": "HJhJuCQbDKd0rsDwKEE8iA",
                    "children": []
                  },
                  {
                    "id": "22",
                    "parent": "19",
                    "text": "Diuréticos (1)",
                    "guid": "F50jkX5YYztzUxEMG040EA",
                    "children": []
                  }
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "23",
        "parent": "0",
        "text": "FASES VOLUMEN\nURINARIO POST-QX",
        "guid": "",
        "children": [
          {
            "id": "24",
            "parent": "23",
            "text": "→ Día 0-5: DI inicial",
            "guid": "Yp5BcQltB7tQcsLkfwjZHg",
            "children": [
              {
                "id": "25",
                "parent": "24",
                "text": "Shock axonal → ↓ADH",
                "guid": "A1rSSheMpRBZqj9GF70JDQ",
                "children": []
              }
            ]
          },
          {
            "id": "26",
            "parent": "23",
            "text": "→ Día 6-11: Interfase antidiurética",
            "guid": "flB5uzFCKOU0HhARCa8_hQ",
            "children": [
              {
                "id": "27",
                "parent": "26",
                "text": "Degeneración neuronal → inercia antidiurética",
                "guid": "Drl5NUOMUsUZSMcnXercJw",
                "children": []
              }
            ]
          },
          {
            "id": "28",
            "parent": "23",
            "text": "→ Día 12-14: DI posterior",
            "guid": "ZMLVTWqtFWkHisnGL5rrBQ",
            "children": [
              {
                "id": "29",
                "parent": "28",
                "text": "Agotamiento ADH almacenada hipófisis post (1)",
                "guid": "EriHABrLXqRiycIvL1DQuQ",
                "children": []
              }
            ]
          }
        ]
      },
      {
        "id": "30",
        "parent": "0",
        "text": "CRITERIOS DX DI\nPOSTOP (1)",
        "guid": "",
        "children": [
          {
            "id": "31",
            "parent": "30",
            "text": "→ Diuresis >500 cm³/2h consecutivas",
            "guid": "VFImCEJEhdE3eADqUqKfxA",
            "children": []
          },
          {
            "id": "32",
            "parent": "30",
            "text": "→ Na⁺ plasmático >145 mEq/L",
            "guid": "c4autmuWEPxcUd9JWEmEBA",
            "children": []
          },
          {
            "id": "33",
            "parent": "30",
            "text": "→ Glicemia <160 mg/dL",
            "guid": "VkMmZWPcqQ8H5G8JWZ34rw",
            "children": []
          }
        ]
      },
      {
        "id": "34",
        "parent": "0",
        "text": "PREDICTORES\nDI (2,5,6)",
        "guid": "",
        "children": [
          {
            "id": "35",
            "parent": "34",
            "text": "→ Qx microadenomas",
            "guid": "e2lOH2GOrk9xWwG9CiLHVQ",
            "children": []
          },
          {
            "id": "36",
            "parent": "34",
            "text": "→ ↓LCR intraop",
            "guid": "JRsBFAqjyORoDaN8N9OIFQ",
            "children": []
          },
          {
            "id": "37",
            "parent": "34",
            "text": "→ Lesiones\nextraselares:",
            "guid": "JW8niUrXZatnJFjOZFWS1A",
            "children": [
              {
                "id": "38",
                "parent": "37",
                "text": "Craneofaringioma",
                "guid": "Ik_oyj62Zg862SGxeCDPJA",
                "children": []
              },
              {
                "id": "39",
                "parent": "37",
                "text": "Quiste bolsa Rathke",
                "guid": "CZuEXSyq5J8wj_bwXe2qZQ",
                "children": []
              }
            ]
          },
          {
            "id": "40",
            "parent": "34",
            "text": "→ Edad joven",
            "guid": "bu9qcGgH99swkEopYnYZJw",
            "children": []
          },
          {
            "id": "41",
            "parent": "34",
            "text": "→ Sexo masculino",
            "guid": "U54I1wziKXI6v50rKeEvPA",
            "children": []
          },
          {
            "id": "42",
            "parent": "34",
            "text": "→ Enf. Cushing",
            "guid": "cL7SgkKkDDQDfyfrbCggoQ",
            "children": []
          },
          {
            "id": "43",
            "parent": "34",
            "text": "→ Reoperación lesión selar",
            "guid": "JDK6g3TaKah2Suf2SU27lw",
            "children": []
          }
        ]
      },
      {
        "id": "44",
        "parent": "0",
        "text": "PROTOCOLO\nMONITORIZACIÓN",
        "guid": "f33yjV5Yi3IBIUOsJO0aFg",
        "children": [
          {
            "id": "45",
            "parent": "44",
            "text": "Día 1\npostQx:",
            "guid": "S_ek6Hzr0_1Zt8_qPLZ3ag",
            "children": [
              {
                "id": "46",
                "parent": "45",
                "text": "→ Diuresis c/4h (sin sonda Foley)",
                "guid": "KS_xHWhFnHoJQqzqS3_Z5w",
                "children": []
              },
              {
                "id": "47",
                "parent": "45",
                "text": "→ Si diuresis\n>500 cm³/2h:",
                "guid": "JvwCikQbzpxDoKkMMJeG5w",
                "children": [
                  {
                    "id": "48",
                    "parent": "47",
                    "text": "Medir glicemia",
                    "guid": "cMazO3Qwn_wOhTFMX7Ydqw",
                    "children": []
                  },
                  {
                    "id": "49",
                    "parent": "47",
                    "text": "Medir Na⁺ plasmático",
                    "guid": "XDiX1z8Ve3ZCLDbSL9agrg",
                    "children": []
                  }
                ]
              }
            ]
          },
          {
            "id": "50",
            "parent": "44",
            "text": "Día 2:",
            "guid": "P4_gMV0299Yo3pgMY8KatA",
            "children": [
              {
                "id": "51",
                "parent": "50",
                "text": "→ ↓Hidratación parenteral",
                "guid": "UhEhfh8pgAItEFZMUY8UCw",
                "children": []
              },
              {
                "id": "52",
                "parent": "50",
                "text": "→ Diuresis c/6h",
                "guid": "fYILdS4xmfh2fC4hJrH8kg",
                "children": []
              },
              {
                "id": "53",
                "parent": "50",
                "text": "→ Si >1000 cm³/6h:",
                "guid": "Fnc2c3--2wxyMdZ6PXM4_Q",
                "children": [
                  {
                    "id": "54",
                    "parent": "53",
                    "text": "Medir glicemia",
                    "guid": "Q9qpqDXSf4ZuCr_kNKFc4w",
                    "children": []
                  },
                  {
                    "id": "55",
                    "parent": "53",
                    "text": "Medir Na⁺ plasmático",
                    "guid": "KgMfgnyP8TAUV3qOBju3Wg",
                    "children": []
                  }
                ]
              },
              {
                "id": "56",
                "parent": "50",
                "text": "→ Si DI (+): mantener protocolo c/6h",
                "guid": "O6VsplaDsWE2ElgIB50Rjw",
                "children": []
              },
              {
                "id": "57",
                "parent": "50",
                "text": "→ Si DI (-): control c/24h desde día 3",
                "guid": "U2-FXg_KJ_NEU4j5Ev9lkA",
                "children": []
              }
            ]
          },
          {
            "id": "58",
            "parent": "44",
            "text": "DÍA 3 HASTA\nALTA",
            "guid": "ajOQ-z7lLfRTy49DPaMWWQ",
            "children": [
              {
                "id": "59",
                "parent": "58",
                "text": "→ Na⁺ plasmático matinal",
                "guid": "BtxegCLBf4kl7l_5SrcIKA",
                "children": []
              },
              {
                "id": "60",
                "parent": "58",
                "text": "→ Diuresis c/24h",
                "guid": "WJP_DxP5H91_WGULApcekg",
                "children": []
              },
              {
                "id": "61",
                "parent": "58",
                "text": "→ Asegurar acceso libre agua",
                "guid": "EIkRDROv35oI0tXsTC59tA",
                "children": []
              }
            ]
          },
          {
            "id": "62",
            "parent": "44",
            "text": "TRATAMIENTO DI",
            "guid": "DFuhCCPSVpAQoVVTCd2sfQ",
            "children": [
              {
                "id": "63",
                "parent": "62",
                "text": "→ DDAVP IV dosis única: 4 μg",
                "guid": "Tq9V5xgfGDxQonvpO7B1sA",
                "children": []
              },
              {
                "id": "64",
                "parent": "62",
                "text": "→ Preparación:",
                "guid": "X51qGWNtW3RKBs0BcCZ7Jw",
                "children": [
                  {
                    "id": "65",
                    "parent": "64",
                    "text": "Ampolla: 15 μg/1 mL (Octostim®)",
                    "guid": "BVE5QkdkfC56wrb4EazaSg",
                    "children": []
                  },
                  {
                    "id": "66",
                    "parent": "64",
                    "text": "Dilución: hasta\n15 mL SF",
                    "guid": "azbSvgtkDEsbiobHPTrDRw",
                    "children": [
                      {
                        "id": "67",
                        "parent": "66",
                        "text": "Concentración final: 1 μg/mL",
                        "guid": "EoGPv0waL9YltepDGV3uQA",
                        "children": []
                      },
                      {
                        "id": "68",
                        "parent": "66",
                        "text": "Administrar: 4 mL IV",
                        "guid": "btuvYEukSjxkFPZoR2-ubw",
                        "children": []
                      }
                    ]
                  }
                ]
              },
              {
                "id": "69",
                "parent": "62",
                "text": "→ Duración efecto: 6-8h",
                "guid": "dx07DlLZou08VPjbYVDMCQ",
                "children": []
              },
              {
                "id": "70",
                "parent": "62",
                "text": "→ Mantener acceso libre agua",
                "guid": "KD-gWm-WXkwcHpx6LZDZnA",
                "children": []
              }
            ]
          },
          {
            "id": "71",
            "parent": "44",
            "text": "SEGUIMIENTO\nPOST-DDAVP",
            "guid": "Eb7Q4hAgiB4e8-JiYG4myQ",
            "children": [
              {
                "id": "72",
                "parent": "71",
                "text": "→ Control diuresis\nsegún momento dx:",
                "guid": "NvraehbhU3I_PbPmIjGtOA",
                "children": [
                  {
                    "id": "73",
                    "parent": "72",
                    "text": "Día 1 postQx: c/2h",
                    "guid": "IkVfvVrIOq1fbHCANMbvfA",
                    "children": []
                  },
                  {
                    "id": "74",
                    "parent": "72",
                    "text": "Después día 1: c/6h",
                    "guid": "JuJqhAUiWsNOJN28Fb4Z5A",
                    "children": []
                  }
                ]
              },
              {
                "id": "75",
                "parent": "71",
                "text": "→ Si reaparece poliuria:",
                "guid": "UMalADI51CVdLchTMGQPGQ",
                "children": [
                  {
                    "id": "76",
                    "parent": "75",
                    "text": "Medir Na⁺ plasmático",
                    "guid": "FacvmSc0lVUgiopADMRqpw",
                    "children": []
                  },
                  {
                    "id": "77",
                    "parent": "75",
                    "text": "Medir glicemia",
                    "guid": "eg44QlzfgxtuFTawC80JJA",
                    "children": []
                  },
                  {
                    "id": "78",
                    "parent": "75",
                    "text": "Ajustar conducta según resultados",
                    "guid": "bQALOQ0JGRJsOy_uFT-rlA",
                    "children": []
                  }
                ]
              }
            ]
          },
          {
            "id": "79",
            "parent": "44",
            "text": "MANEJO DI\nHOSPITALARIA",
            "guid": "",
            "children": [
              {
                "id": "80",
                "parent": "79",
                "text": "→ Si poliuria persiste: DDAVP oral",
                "guid": "fJ93XghZzGhC0IUwM5pR2A",
                "children": []
              },
              {
                "id": "81",
                "parent": "79",
                "text": "→ Si reaparece poliuria:",
                "guid": "Hzsf2gIOORdVy_8RQYB_lw",
                "children": [
                  {
                    "id": "82",
                    "parent": "81",
                    "text": "Control Na⁺ plasmático",
                    "guid": "XNZzxDU4b5F2R28TA7jeSA",
                    "children": []
                  },
                  {
                    "id": "83",
                    "parent": "81",
                    "text": "Control glicemia",
                    "guid": "OlrKVERsTNAZdvgsCyFvVA",
                    "children": []
                  },
                  {
                    "id": "84",
                    "parent": "81",
                    "text": "Ajustar según resultados",
                    "guid": "dqYg9XakwIA7hX5uDE1Qjg",
                    "children": []
                  }
                ]
              }
            ]
          },
          {
            "id": "85",
            "parent": "44",
            "text": "CONTROL\nPOST-ALTA",
            "guid": "HdlV1VwQCK4ZEbs1F-eOFw",
            "children": [
              {
                "id": "86",
                "parent": "85",
                "text": "→ ELP control día 7:",
                "guid": "OO-Lygcm8eUjtJc8Je-XAw",
                "children": [
                  {
                    "id": "87",
                    "parent": "86",
                    "text": "Riesgo SSIADH aislado/trifásico",
                    "guid": "FDAK9w_vxyo7L0KXEM-CVg",
                    "children": []
                  },
                  {
                    "id": "88",
                    "parent": "86",
                    "text": "Si DDAVP oral: suspender noche previa",
                    "guid": "GEmTkn3_x8hEadQuN4SzbA",
                    "children": []
                  }
                ]
              },
              {
                "id": "89",
                "parent": "85",
                "text": "→ Educar paciente:",
                "guid": "AA4A3xo10z95BTMDXOR0ow",
                "children": [
                  {
                    "id": "90",
                    "parent": "89",
                    "text": "Signos DI/SSIADH",
                    "guid": "T25C0G9MohZgnVLsCckNJQ",
                    "children": []
                  },
                  {
                    "id": "91",
                    "parent": "89",
                    "text": "Vigilar polidipsia/poliuria",
                    "guid": "M7ju5noUSxgU6nx5Kl8P2w",
                    "children": []
                  },
                  {
                    "id": "92",
                    "parent": "89",
                    "text": "Atención especial nicturia",
                    "guid": "cLkLmFBv-uc2rGBpDpJhbQ",
                    "children": []
                  }
                ]
              },
              {
                "id": "93",
                "parent": "85",
                "text": "→ Mantener acceso libre agua",
                "guid": "LIADlk--G54mee-FZW-PYA",
                "children": []
              }
            ]
          },
          {
            "id": "94",
            "parent": "44",
            "text": "DI PERMANENTE\n(1.4-3.4%)",
            "guid": "VuUNg0ouhsELXyZjaxUYew",
            "children": [
              {
                "id": "95",
                "parent": "94",
                "text": "→ Opciones terapéuticas:",
                "guid": "Wh5N60aOaPt75JrRcmfhfQ",
                "children": [
                  {
                    "id": "96",
                    "parent": "95",
                    "text": "a) DDAVP\noral:",
                    "guid": "RI4ww0BObv8p7JTpRJwxog",
                    "children": [
                      {
                        "id": "97",
                        "parent": "96",
                        "text": "0.1 mg x 30 comp",
                        "guid": "WoRCPyLxx-whgKZFKfKFDw",
                        "children": []
                      },
                      {
                        "id": "98",
                        "parent": "96",
                        "text": "0.2 mg x 30 comp",
                        "guid": "Ej5qAgId-TEzu5I0RfdY6Q",
                        "children": []
                      }
                    ]
                  },
                  {
                    "id": "99",
                    "parent": "95",
                    "text": "b) DDAVP\nspray nasal:",
                    "guid": "fDJESkimDq5wVmjEbOtP4g",
                    "children": [
                      {
                        "id": "100",
                        "parent": "99",
                        "text": "5 mL (refrigerado)",
                        "guid": "GRYJlScCyS57fbFQRZYNKw",
                        "children": []
                      },
                      {
                        "id": "101",
                        "parent": "99",
                        "text": "1 puff = 0.1 mL = 10 μg",
                        "guid": "dsDkzCH3oNUrBZyLTaXyUA",
                        "children": []
                      }
                    ]
                  },
                  {
                    "id": "102",
                    "parent": "95",
                    "text": "c) DDAVP\nsublingual:",
                    "guid": "bCYnljZkwu84uwrLRkR1gQ",
                    "children": [
                      {
                        "id": "103",
                        "parent": "102",
                        "text": "120 mg",
                        "guid": "fPMr6jSfpZw4rFb-QYFcrQ",
                        "children": []
                      }
                    ]
                  }
                ]
              },
              {
                "id": "104",
                "parent": "94",
                "text": "→ Ajustar dosis:",
                "guid": "Kbwtxi0gbh5dwHa-Je5yEA",
                "children": [
                  {
                    "id": "105",
                    "parent": "104",
                    "text": "Objetivo: control nicturia",
                    "guid": "dO4Um2KY6-cGHY5PT3JW2g",
                    "children": []
                  },
                  {
                    "id": "106",
                    "parent": "104",
                    "text": "Según necesidad/acceso agua",
                    "guid": "BYqz0yeeNJR5ZNvqF8kd1Q",
                    "children": []
                  }
                ]
